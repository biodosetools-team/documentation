month = {mar},
number = {1},
pages = {33--51},
title = {{An analysis of the distribution and dose response of chromosome aberrations in human lymphocytes after in vitro exposure to137Cesium gamma radiation}},
url = {http://link.springer.com/10.1007/BF01323759},
volume = {22},
year = {1983}
