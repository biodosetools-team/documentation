keywords = {Data analysis,Radiation accidents,Radiation dosimetry,Statistical,Triage},
number = {3},
pages = {307--312},
pmid = {19861736},
title = {{Biodosimetric tools for a fast triage of people accidentally exposed to ionizing radiation. Statistical and computational aspects}},
volume = {45},
year = {2009}
}
